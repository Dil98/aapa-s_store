    <main role="main">
      <!-- New Hero section -->
      <section class="px-5 py-6 py-xxl-10 hcf-bp-center hcf-bs-cover hcf-overlay hcf-transform" style="background-image: url('assets/hero.jpg');">
        <div class="container">
          <div class="row justify-content-md-center">
